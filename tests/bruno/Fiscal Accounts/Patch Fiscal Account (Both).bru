meta {
  name: Patch Fiscal Account (Both)
  type: http
  seq: 9
}

patch {
  url: {{BASEURL}}/api/v1/fiscal_accounts/{{accountId}}
  body: json
  auth: none
}

body:json {
  {
    "account_number": "12345789",
    "type": "SavingsAccount"
  }
}

vars:pre-request {
  accountId: {{REQ_ACCOUNT_ID}}
}

assert {
  res.status: eq 200
  res.body.type: eq SavingsAccount
  res.body.account_number: eq "12345789"
}
