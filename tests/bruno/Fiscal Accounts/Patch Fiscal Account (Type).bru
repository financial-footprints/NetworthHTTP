meta {
  name: Patch Fiscal Account (Type)
  type: http
  seq: 7
}

patch {
  url: {{BASEURL}}/api/v1/fiscal_accounts/{{accountId}}
  body: json
  auth: none
}

body:json {
  {
    "type": "CreditCard"
  }
}

vars:pre-request {
  accountId: {{REQ_ACCOUNT_ID}}
}

assert {
  res.status: eq 200
  res.body.type: eq CreditCard
  res.body.account_number: eq "123456789"
}
