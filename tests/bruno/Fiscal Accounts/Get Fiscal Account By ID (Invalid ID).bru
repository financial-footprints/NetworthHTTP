meta {
  name: Get Fiscal Account By ID (Invalid ID)
  type: http
  seq: 4
}

get {
  url: {{BASEURL}}/api/v1/fiscal_accounts/{{accountId}}
  body: none
  auth: none
}

vars:pre-request {
  accountId: 00000000-0000-0000-0000-000000000000
}

assert {
  res.status: eq 404
  res.body: eq error.fiscal_accounts.get_account.not_found
}
