meta {
  name: Patch Fiscal Account (Account Number)
  type: http
  seq: 8
}

patch {
  url: {{BASEURL}}/api/v1/fiscal_accounts/{{accountId}}
  body: json
  auth: none
}

body:json {
  {
    "account_number": "92345789"
  }
}

vars:pre-request {
  accountId: {{REQ_ACCOUNT_ID}}
}

assert {
  res.status: eq 200
  res.body.type: eq CreditCard
  res.body.account_number: eq "92345789"
}
